<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="UWOsh_InteractiveMap.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:UWOsh_InteractiveMap"
    >
       
 <!--Code by Shabbar Kazmi -->
 <!--Reviewed by Rudy Liljeberg -->
    <TabBar>
    <Tab Title="MapTab" Icon="mapicon.svg">
            <ShellContent ContentTemplate="{DataTemplate local:MapUwosh}"/>
        </Tab>
        
        <Tab Title="CompassTab" Icon="compassicon.svg">
            <ShellContent ContentTemplate="{DataTemplate local:Compass}"/>
        </Tab>

        <Tab Title="ListAllPlantsTab" Icon="plantslist.svg">
            <ShellContent ContentTemplate="{DataTemplate local:ListAllPlants}"/>
        </Tab>

        <Tab Title="SearchHistory" Icon="plantslist.svg">
            <ShellContent ContentTemplate="{DataTemplate local:SearchHistory}"/>
        </Tab>
    </TabBar>
</Shell>
